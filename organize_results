                            printf "," >> $MASK_FOLDER/segmentation_FreeSurfer.csv
                            #FMEASR
                            printf "`cat $MASK_FOLDER/$file | grep FMEASR | awk '{print $3}'`" >> $MASK_FOLDER/segmentation_FreeSurfer.csv
                              printf "," >> $MASK_FOLDER/segmentation_FreeSurfer.csv
                              #ACURCY
                              printf "`cat $MASK_FOLDER/$file | grep ACURCY | awk '{print $3}'`" >> $MASK_FOLDER/segmentation_FreeSurfer.csv
