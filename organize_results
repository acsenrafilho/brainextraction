    printf "`cat $MASK_FOLDER/$file | grep ICCORR | awk '{print $3}'`" >> $MASK_FOLDER/segmentation_FreeSurfer.csv
      printf "," >> $MASK_FOLDER/segmentation_FreeSurfer.csv
